    "@types/sharp": "^0.31.1",
    "better-sqlite3": "^8.1.0",
    "content-disposition": "^0.5.4",
    "dotenv": "^16.0.3",
    "drizzle-orm": "^0.21.1",
    "execa": "^7.0.0",
    "fast-deep-equal": "^3.1.3",

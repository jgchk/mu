  "devDependencies": {
    "@types/node": "^18.15.3",
    "chokidar-cli": "^3.0.0",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "execa": "^7.1.1",
    "zod": "^3.21.4"
  }
}
